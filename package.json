{
  "name": "react-minimalistic-gantt",
  "description": "A minimalistic gantt component for react",
  "repository": "https://github.com/knalum/react-minimalistic-gantt",
  "homepage": "https://github.com/knalum/react-minimalistic-gantt",
  "files": [
    "/dist/index.es.js",
    "/dist/component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/component/index.d.ts"
    }
  },
  "version": "0.0.12",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build2": "vite --config vite.config.demo.ts build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@uidotdev/usehooks": "^2.4.1",
    "vite-plugin-dts": "^3.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
